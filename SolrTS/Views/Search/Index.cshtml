@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="py-5 text-center" >
     <h1>Search Me</h1>
     </div>
    <div class="row">
        
        <div class="col-md-4 order-md-1 mb-4">

        </div>
        <div class="col-md-4 order-md-2 mb-4">
            

            <input type="text" id="SearchBox" class="form-control" onkeyup="SearchMe()" placeholder="Type Name to Search" />


            <div style="margin:1em; font:bold;" id="SearchResults">
                Search Results Appear here.
            </div>

            <form class="card p-2">
                <div class="input-group">
                    <input type="text" class="form-control" id="AddMe" placeholder="Type Name">
                    <div class="input-group-append">
                        <button class="btn btn-secondary" onclick="AddMeButton()">Add</button>
                    </div>
                </div>
            </form>

            <div style="margin:1em; font:bold;" id="AddResults">
                Click above to add users.
            </div>
        </div>
    </div>

